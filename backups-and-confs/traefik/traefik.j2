
providers:
  docker:
    endpoint: "unix:///var/run/docker.sock"
  file:
    filename: "/etc/traefik/dynamic.yml"
    watch: true

entryPoints:
  web:
    address: ":80"
  websecure:
    address: ":443"

certificatesResolvers:
  master:
    acme:
      email: "{{ acme_email }}"
      storage: "/etc/traefik/acme.json"
      httpChallenge:
        entryPoint: web

api:
  insecure: true